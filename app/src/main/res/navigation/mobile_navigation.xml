        />

    <activity
        android:id="@+id/navigation_about"
        android:name="com.example.c4u2.about_us_Main2Activity"
        android:label="About Us"
        tools:layout="@layout/activity_about_us__main2"

        />



    <activity
        android:id="@+id/navigation_my_account"
        android:name="com.example.c4u2.MyAcctActivity"
        android:label="My Account"
