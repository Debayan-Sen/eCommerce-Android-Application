        tools:layout="@layout/fragment_painting" />




    <activity
        android:id="@+id/navigation_order"
        android:name="com.example.c4u2.my_order_Main2Activity"
        android:label="My Order"
        tools:layout="@layout/activity_my_order__main2"

        />

    <activity
        android:id="@+id/navigation_cart"
        android:name="com.example.c4u2.my_cart_Main2Activity"
